    <div class="mx-auto text-center flex items-center flex-col gap-2">
      <Header />
      <div class="flex flex-row gap-1">
        <a href="https://instagram.com/bellezzadi_em">
          <Icon name="mdi:instagram" class="text-yellow-700" size={40} />
        </a>
		<a href="mailto:hello@bellezzadiem.fr">
			<Icon name="mdi:at" class="text-yellow-700" size={40} />
		  </a>
      </div>
    </div>
  </main>
</Layout>
